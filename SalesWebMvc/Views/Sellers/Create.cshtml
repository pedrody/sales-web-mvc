@model SalesWebMvc.Models.Seller

@{
    ViewData["Title"] = "Create";
}

<h1>@ViewData["Title"]</h1>

<h4>Seller</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div>
                <label asp-for="Name" class="form-label mt-4"></label>
                <input asp-for="Name" class="form-control">
            </div>
            <div>
                <label asp-for="Email" class="form-label mt-4"></label>
                <input asp-for="Email" class="form-control">
            </div>
            <div>
                <label asp-for="BirthDate" class="form-label mt-4"></label>
                <input asp-for="BirthDate" class="form-control">
            </div>
            <div>
                <label asp-for="Salary" class="form-label mt-4"></label>
                <input asp-for="Salary" class="form-control">
            </div>
            <div>
                <input type="submit" value="Create" class="btn btn-success mt-4">
            </div>

        </form>
    </div>
</div>